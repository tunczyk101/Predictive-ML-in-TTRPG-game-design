endium.pf2e.conditionitems.fesd1n5eVhpCSS18]{Sickened 2} on a critical hit). This condition cannot be reduced below 1 while the creature is subject to the persistent fire damage from the attack, and ends automatically when the persistent fire ends.</p>"},"requirements":{"value":""},"rules":[],"slug":null,"source":{"value":""},"traits":{"rarity":"common","value":[]},"trigger":{"value":""},"weapon":{"value":""},"schema":{"version":0.827,"lastMigration":null}},"type":"action"},{"_id":"77NUSmjyQXHh7j0h","img":"systems/pf2e/icons/default-icons/lore.svg","name":"Acrobatics","sort":700000,"system":{"description":{"value":""},"mod":{"value":10},"proficient":{"value":0},"rules":[],"slug":null,"source":{"value":""},"traits":{"rarity":"common","value":[]},"schema":{"version":0.827,"lastMigration":null}},"type":"lore"}],"name":"Corrupted Wildfire (3-4)","prototypeToken":{"name":"Corrupted Wildfire"},"system":{"abilities":{"cha":{"mod":-2},"con":{"mod":4},"dex":{"mod":4},"int":{"mod":-2},"str":{"mod":3},"wis":{"mod":3}},"attributes":{"ac":{"details":"","value":18},"allSaves":{"value":""},"hp":{"details":"","max":51,"temp":0,"value":51},"immunities":[{"type":"paralyzed"},{"type":"bleed"},{"type":"fire"},{"type":"poison"},{"type":"sleep"}],"initiative":{"ability":"perception"},"perception":{"value":7},"speed":{"otherSpeeds":[],"value":50},"weaknesses":[{"type":"cold","value":4}]},"details":{"alignment":{"value":"CE"},"blurb":"","creatureType":"","level":{"value":3},"privateNotes":"","publicNotes":"","source":{"value":"Pathfinder Society Scenario #2-14: Lost in Flames"}},"resources":{},"saves":{"fortitude":{"saveDetail":"","value":12},"reflex":{"saveDetail":"","value":9},"will":{"saveDetail":"","value":8}},"traits":{"attitude":{"value":"hostile"},"languages":{"custom":"","selected":[],"value":["abyssal","ignan"]},"rarity":"uncommon","senses":{"value":"darkvision, smoke vision"},"size":{"value":"med"},"value":["fire","elemental"]},"schema":{"version":0.827,"lastMigration":null}},"ty