{"_id":"1VuHjj32wge2gPOr","img":"systems/pf2e/icons/spells/animal-feature.webp","name":"Spell Effect: Animal Feature (Wings)","system":{"description":{"value":"<p>Granted by <em>@UUID[Compendium.pf2e.spells-srd.vGEgI8e7AW6FQ3tP]{Animal Feature}</em>.</p>\n<p>You gain a Fly Speed equal to your land Speed.</p>"},"duration":{"expiry":"turn-start","sustained":false,"unit":"minutes","value":1},"level":{"value":4},"rules":[{"key":"BaseSpeed","selector":"fly","value":"@actor.attributes.speed.total"}],"source":{"value":"Pathfinder Advanced Player's Guide"},"start":{"initiative":null,"value":0},"target":null,"tokenIcon":{"show":true},"traits":{"rarity":"common","value":[]},"slug":"spell-effect-animal-feature-wings","schema":{"version":0.827,"lastMigration":null}},"type":"effect","flags":{"core":{"sourceId":"Compendium.pf2e.spell-effects.1VuHjj32wge2gPOr"}}}
{"_id":"1gTnFmfLUQnCo3TK","img":"systems/pf2e/icons/spells/dread-ambience.webp","name":"Spell Effect: Dread Ambience (Success)","system":{"badge":null,"description":{"value":"<p>Every creature in the area takes a -1 status penalty to saving throws against fear effects.</p>"},"duration":{"expiry":"turn-start","sustained":false,"unit":"unlimited","value":-1},"level":{"value":5},"rules":[{"key":"FlatModifier","predicate":["fear"],"selector":"saving-throw","type":"status","value":-1}],"source":{"value":"Pathfinder Secrets of Magic"},"start":{"initiative":null,"value":0},"target":null,"tokenIcon":{"show":true},"traits":{"rarity":"common","value":[]},"slug":"spell-effect-dread-ambience-success","schema":{"version":0.827,"lastMigration":null}},"type":"effect","flags":{"core":{"sourceId":"Compendium.pf2e.spell-effects.1gTnFmfLUQnCo3TK"}}}
