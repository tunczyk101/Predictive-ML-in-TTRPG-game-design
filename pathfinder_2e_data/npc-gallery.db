value":""},"attackEffects":{"value":[]},"bonus":{"value":12},"damageRolls":{"0":{"damage":"1d6+2","damageType":"piercing"}},"description":{"value":""},"rules":[],"slug":null,"source":{"value":""},"traits":{"rarity":"common","value":["deadly-d10","range-increment-60","reload-0"]},"weaponType":{"value":"ranged"},"schema":{"version":0.827,"lastMigration":null}},"type":"melee"},{"_id":"YB9T72k74oWO9L6c","flags":{"core":{"sourceId":"Compendium.pf2e.bestiary-ability-glossary-srd.fFu2sZz4KB01fVRc"}},"img":"systems/pf2e/icons/actions/Passive.webp","name":"Low-Light Vision","sort":1100000,"system":{"actionCategory":{"value":"interaction"},"actionType":{"value":"passive"},"actions":{"value":null},"description":{"value":"<p>@Localize[PF2E.NPC.Abilities.Glossary.LowLightVision]</p>"},"requirements":{"value":""},"rules":[],"slug":"low-light-vision","source":{"value":"Pathfinder Bestiary"},"traits":{"rarity":"common","value":[]},"trigger":{"value":""},"weapon":{"value":""},"schema":{"version":0.827,"lastMigration":null}},"type":"action"},{"_id":"agaJs5LEhgmy0sPO","img":"systems/pf2e/icons/actions/Reaction.webp","name":"Scout's Warning","sort":1200000,"system":{"actionCategory":{"value":"interaction"},"actionType":{"value":"reaction"},"actions":{"value":null},"description":{"value":"<p><strong>Trigger</strong> The field agent is about to roll a Perception check for initiative.</p>\n<hr />\n<p><strong>Effect</strong> The field agent audibly or visually warns their allies of imminent danger. This ability gains the auditory trait if the warning is audible, and the visual trait if the warning is visual. Any ally who is able to hear the agent (if the warning is audible) or see the agent (if the warning is visual) gains a +1 circumstance bonus to their initiative roll.</p>\n<p>@UUID[Compendium.pf2e.feat-effects.RxDDXK52lwyHXl7v]{Effect: Scout's Warning}</p>"},"requirements":{"value":""},"rules":[],"slug":null,"source":{"value":""},"traits":{"rarity":"common","value":[]},"trigger":{"valu