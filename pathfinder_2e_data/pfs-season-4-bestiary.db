s":"","value":24},"adjustment":null,"allSaves":{"value":""},"hp":{"details":"coffin restoration, fast healing 7, negative healing","max":65,"temp":0,"value":65},"immunities":[{"type":"death-effects"},{"type":"disease"},{"type":"paralyzed"},{"type":"poison"},{"type":"sleep"}],"initiative":{"ability":"perception"},"perception":{"value":17},"resistances":[{"exceptions":["magical","silver"],"type":"physical","value":7}],"speed":{"otherSpeeds":[{"type":"climb","value":25}],"value":25},"weaknesses":[{"type":"vampire-weaknesses","value":0}]},"details":{"alignment":{"value":"CE"},"blurb":"","creatureType":"Undead","level":{"value":6},"privateNotes":"","publicNotes":"<p>Vampire counts rule their demesnes through a mix of fear and cruelty.</p>\n<hr />\n<p>Vampires are undead creatures that feed on the blood of the living.</p>","source":{"value":"Pathfinder Society Scenario #4-09: Killer in the Golden Mask"}},"resources":{},"saves":{"fortitude":{"saveDetail":"","value":11},"reflex":{"saveDetail":"","value":14},"will":{"saveDetail":"","value":17}},"traits":{"languages":{"custom":"","selected":[],"value":["common","necril","varisian"]},"rarity":"common","senses":{"value":"darkvision"},"size":{"value":"med"},"value":["undead","vampire"]},"schema":{"version":0.827,"lastMigration":null}},"type":"npc","flags":{"core":{"sourceId":"Compendium.pf2e.pfs-season-4-bestiary.9TWGnI8mAUwt4jqE"}}}
